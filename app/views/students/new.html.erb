<!-- Create your form here -->
<h1>Student Form</h1>
<%= form_tag '/students', method: "post" do %>
  <%= label_tag(:'first_name', "First Name")%>
  <%= text_field_tag(:'first_name', nil, id: "first_name")%>
  <%= label_tag(:'last_name', "Last Name")%>
  <%= text_field_tag(:'last_name', nil, id: "last_name")%>
  <%= submit_tag("Submit Student")%>
<% end %>
