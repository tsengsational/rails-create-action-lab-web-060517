<!-- Create your form here -->
<% form_tag students_path, method: "POST" do %>
  <%= label_tag(:'student[first_name]', "First Name")%>
  <%= text_field_tag(:'student[first_name]',nil, id: "first_name")%>
  <%= label_tag(:'student[last_name]', "Last Name")%>
  <%= text_field_tag(:'student[last_name]',nil, id: "last_name")%>
  <%= submit_tag("Create Student")%>
<% end %>
